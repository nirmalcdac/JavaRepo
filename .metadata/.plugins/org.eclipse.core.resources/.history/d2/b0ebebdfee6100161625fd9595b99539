public class Prectice 
{
	public static void main(String[] args) 
	{
		System.out.println("hello java");
		Class1 cl=new Class1();
		Cow c=new Cow();
		c.eat();
		Human h= new Human();
		h.eat();
		System.out.println(cl.a);
	}
}
