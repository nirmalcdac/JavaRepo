public abstract class Account implements AccountInterface
{
	int accountnumber;
	int balance;
	/* (non-Javadoc)
	 * @see AccountInterface#getAccountnumber()
	 */
	public int getAccountnumber()
	{
		return accountnumber;
	}
	/* (non-Javadoc)
	 * @see AccountInterface#setAccountnumber(int)
	 */
	public void setAccountnumber(int accountnumber)
	{
		this.accountnumber = accountnumber;
	}
	/* (non-Javadoc)
	 * @see AccountInterface#getBalance()
	 */
	public int getBalance()
	{
		return balance;
	}
	/* (non-Javadoc)
	 * @see AccountInterface#setBalance(int)
	 */
	public void setBalance(int balance)
	{
		this.balance = balance;
	}
	/* (non-Javadoc)
	 * @see AccountInterface#deposit(int)
	 */
	public void deposit(int ammount)
	{
		this.balance = this.balance+ammount;
	}
	/* (non-Javadoc)
	 * @see AccountInterface#withdraw(int)
	 */
	public abstract void withdraw(int ammount);
	{
		//this.balance = this.balance-ammount;
	}
}